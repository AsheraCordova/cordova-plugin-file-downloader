<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="cordova-plugin-file-downloader" version="0.4.2">
	<name>Downloader</name>
	<author>Fabian Strachanski</author>
	<author>Manuel Ramon Lopez Torres</author>
	<author>Martijn Swart</author>
	<description>Downloads files to persistant Storage on your device, checks md5sum if you want, unzips packed files if necassary (For Android and iOS)</description>
	<license>MIT</license>
	<keywords>file,download,checksum,md5,unzip</keywords>
	<repo>https://github.com/StudioKloek/cordova-plugin-file-downloader.git</repo>
	<issue>https://github.com/StudioKloek/cordova-plugin-file-downloader/issues</issue>
	<dependency id="cordova-plugin-file" url="https://github.com/AsheraCordova/cordova-plugin-file.git" />
	<dependency id="cordova-plugin-file-transfer" url="https://github.com/AsheraCordova/cordova-plugin-file-transfer.git" />
	<dependency id="cordova-plugin-network-information" url="https://github.com/AsheraCordova/cordova-plugin-network-information.git" />
	<dependency id="cordova-plugin-zip" url="https://github.com/AsheraCordova/cordova-plugin-zip.git" />
	<dependency id="cordova-plugin-file-md5" version="~0.3.3" />

	<js-module src="src/downloader.js" name="downloader">
		<clobbers target="downloader" />
	</js-module>
</plugin>
